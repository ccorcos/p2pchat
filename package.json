{
	"name": "typescript-boilerplate",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"setup": "mkdir -p dist/renderer && cp src/renderer/index.html dist/renderer/index.html",
		"build": "tsc",
		"watch": "tsc -w",
		"lint": "tslint -c tslint.json -p tsconfig.json",
		"start": "npm run setup && npm run build && electron ./dist/main/index.js",
		"test": "ava"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register"
		]
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"charwise": "^3.0.1",
		"chokidar": "^2.0.4",
		"cuid": "^2.1.4",
		"fs-extra": "^7.0.1",
		"glamor": "^2.20.40",
		"level": "^4.0.0",
		"level-mem": "^3.0.1",
		"lodash": "^4.17.11",
		"moment": "^2.23.0",
		"react": "^16.7.0-alpha.0",
		"react-dom": "^16.7.0-alpha.0",
		"signalhub": "^4.9.0",
		"simple-peer": "^9.2.0",
		"url": "^0.11.0",
		"wrtc": "^0.3.3"
	},
	"devDependencies": {
		"@types/chokidar": "^1.7.5",
		"@types/fs-extra": "^5.0.4",
		"@types/levelup": "^3.1.0",
		"@types/lodash": "^4.14.118",
		"@types/moment": "^2.13.0",
		"@types/react": "^16.7.13",
		"@types/react-dom": "^16.0.11",
		"@types/simple-peer": "^6.1.5",
		"@types/webpack": "^4.4.20",
		"ava": "^1.0.1",
		"awesome-typescript-loader": "^5.2.1",
		"electron": "^3.0.12",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"typescript": "^3.2.2",
		"webpack": "^4.27.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	}
}
