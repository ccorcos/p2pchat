{
	"name": "typescript-boilerplate",
	"version": "1.0.0",
	"description": "boilerplate for web applications using typescript and webpack",
	"main": "index.js",
	"scripts": {
		"setup": "mkdir -p dist/renderer && cp src/renderer/index.html dist/renderer/index.html",
		"build": "tsc",
		"watch": "tsc -w",
		"start": "npm run setup && npm run build && electron ./dist/main/index.js",
		"test": "ava"
	},
	"ava": {
		"compileEnhancements": false,
		"extensions": [
			"ts",
			"tsx"
		],
		"require": [
			"ts-node/register"
		]
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"charwise": "^3.0.1",
		"chokidar": "^3.3.1",
		"cuid": "^2.1.8",
		"fs-extra": "^8.1.0",
		"glamor": "^2.20.40",
		"level": "^6.0.0",
		"level-mem": "^5.0.1",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"signalhub": "^4.9.0",
		"simple-peer": "^9.6.2",
		"url": "^0.11.0",
		"wrtc": "^0.4.3"
	},
	"devDependencies": {
		"@types/chokidar": "^2.1.3",
		"@types/fs-extra": "^8.0.1",
		"@types/levelup": "^4.3.0",
		"@types/lodash": "^4.14.149",
		"@types/moment": "^2.13.0",
		"@types/node": "^13.1.6",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@types/simple-peer": "^9.6.0",
		"@types/webpack": "^4.41.2",
		"ava": "^2.4.0",
		"awesome-typescript-loader": "^5.2.1",
		"electron": "^6.1.4",
		"file-loader": "^5.0.2",
		"html-webpack-plugin": "^3.2.0",
		"ts-node": "^8.6.0",
		"tslint": "^5.20.1",
		"typescript": "^3.7.4",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.10.1"
	}
}
